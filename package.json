{
  "name": "astro-template",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "dev": "astro dev --host",
    "start": "astro dev",
    "build": "astro check && astro build",
    "lefthook:build": "astro check && astro build 2>&1 | grep 'TypeError:' && exit 1 || true",
    "astro:check": "astro check",
    "preview": "astro preview",
    "astro": "astro",
    "prepare": "panda codegen --clean",
    "debug:panda": "panda debug",
    "studio": "panda studio --host --port 4322",
    "check-deps": "ncu",
    "update-deps": "ncu -u",
    "check": "biome check ./src",
    "lint:eslint": "eslint ./src",
    "lint:text": "textlint \"**/*.{md,mdx}\" \"!**/node_modules\"",
    "lint:markup": "markuplint \"src/**/*.{astro,jsx,tsx}\" --problem-only",
    "lint:markdown": "markdownlint-cli2 \"**/*.{md,mdx}\" \"!**/node_modules\""
  },
  "dependencies": {
    "@astrojs/check": "^0.9.4",
    "@astrojs/mdx": "^4.0.6",
    "@astrojs/react": "^4.1.5",
    "@astrojs/sitemap": "^3.2.1",
    "@astrojs/vercel": "^8.0.2",
    "@nanostores/react": "^0.8.4",
    "@textlint/textlint-plugin-markdown": "^14.4.2",
    "@types/react": "^19.0.7",
    "@types/react-dom": "^19.0.3",
    "@vercel/speed-insights": "^1.1.0",
    "astro": "^5.1.7",
    "axios": "^1.7.9",
    "lucide-react": "^0.472.0",
    "nanostores": "^0.11.3",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "typescript": "^5.7.3"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@commitlint/cli": "^19.6.1",
    "@commitlint/config-conventional": "^19.6.0",
    "@markuplint/astro-parser": "^4.6.14",
    "@markuplint/jsx-parser": "^4.7.14",
    "@markuplint/react-spec": "^4.5.14",
    "@pandacss/dev": "^0.51.1",
    "@pandacss/eslint-plugin": "^0.2.4",
    "@pandacss/studio": "^0.51.1",
    "@textlint-ja/textlint-rule-no-insert-dropping-sa": "^2.0.1",
    "@textlint-ja/textlint-rule-no-synonyms": "^1.3.0",
    "@typescript-eslint/parser": "^8.20.0",
    "astro-eslint-parser": "^1.1.0",
    "eslint-plugin-astro": "^1.3.1",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "lefthook": "^1.10.8",
    "markdownlint-cli2": "^0.17.2",
    "markuplint": "^4.11.4",
    "npm-check-updates": "^17.1.14",
    "postcss": "^8.5.1",
    "sharp": "^0.33.5",
    "sudachi-synonyms-dictionary": "^14.0.0",
    "textlint": "^14.4.2",
    "textlint-rule-ja-hiragana-fukushi": "^1.3.0",
    "textlint-rule-ja-hiragana-hojodoushi": "^1.1.0",
    "textlint-rule-ja-hiragana-keishikimeishi": "^1.1.0",
    "textlint-rule-ja-no-orthographic-variants": "^2.0.0",
    "textlint-rule-no-mixed-zenkaku-and-hankaku-alphabet": "^1.0.1",
    "textlint-rule-prefer-tari-tari": "^1.0.3",
    "textlint-rule-preset-ja-technical-writing": "^12.0.2"
  }
}
