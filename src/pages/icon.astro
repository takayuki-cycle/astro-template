---
import Layout from '@/layouts/Layout.astro'
import type { HeadPageProps } from '@/layouts/head/types'
import { css } from '@/../styled-system/css'
import {
  SearchIcon,
  HomeIcon,
  HomeFillIcon,
  MenuIcon,
  CloseIcon,
  SettingsIcon,
  SettingsFillIcon,
  CheckCircleIcon,
  CheckCircleFillIcon,
  FavoriteIcon,
  FavoriteFillIcon,
  AddIcon,
  DeleteIcon,
  DeleteFillIcon,
  ArrowBackIcon,
  StarIcon,
  StarFillIcon,
  ChevronRightIcon,
  LogoutIcon,
  ArrowForwardIOSIcon
} from '@/assets/icons'

const headPage: HeadPageProps = {
  title: 'iconPageTitle',
  description: 'iconページの説明です。',
  robots: 'all',
  ogType: 'website',
  ogImage: '/ogp/general.webp',
  ogImageAlt: 'ogImageAlt',
  xCreator: '@xCreator',
  author: 'author'
}
---

<Layout headPage={headPage}>
  <SearchIcon width='24px' height='24px' class={css({ fill: 'primary' })} />
  <HomeIcon width='22px' height='24px' class={css({ fill: 'primary' })} />
  <HomeFillIcon width='22px' height='24px' class={css({ fill: 'primary' })} />
  <MenuIcon width='24px' height='16px' class={css({ fill: 'primary' })} />
  <CloseIcon width='24px' height='24px' class={css({ fill: 'primary' })} />
  <SettingsIcon width='24px' height='24px' class={css({ fill: 'primary' })} />
  <SettingsFillIcon width='24px' height='24px' class={css({ fill: 'primary' })} />
  <CheckCircleIcon width='24px' height='24px' class={css({ fill: 'primary' })} />
  <CheckCircleFillIcon width='24px' height='24px' class={css({ fill: 'primary' })} />
  <FavoriteIcon width='24px' height='22px' class={css({ fill: 'primary' })} />
  <FavoriteFillIcon width='24px' height='22px' class={css({ fill: 'primary' })} />
  <AddIcon width='24px' height='24px' class={css({ fill: 'primary' })} />
  <DeleteIcon width='21px' height='24px' class={css({ fill: 'primary' })} />
  <DeleteFillIcon width='21px' height='24px' class={css({ fill: 'primary' })} />
  <ArrowBackIcon width='24px' height='24px' class={css({ fill: 'primary' })} />
  <StarIcon width='24px' height='23px' class={css({ fill: 'primary' })} />
  <StarFillIcon width='24px' height='23px' class={css({ fill: 'primary' })} />
  <ChevronRightIcon width='15px' height='24px' class={css({ fill: 'primary' })} />
  <LogoutIcon width='24px' height='24px' class={css({ fill: 'primary' })} />
  <ArrowForwardIOSIcon width='14px' height='24px' class={css({ fill: 'primary' })} />
